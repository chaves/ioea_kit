generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model call_comments {
  id          Int    @id @default(autoincrement()) @db.UnsignedInt
  call_id     Int    @db.UnsignedInt
  reviewer_id Int    @db.UnsignedInt
  comment     String @db.Text
}

model call_notes {
  id          Int @id @default(autoincrement()) @db.UnsignedInt
  note        Int @db.UnsignedSmallInt
  call_id     Int @db.UnsignedInt
  reviewer_id Int @db.UnsignedInt
  type        Int @db.UnsignedTinyInt

  @@index([call_id], map: "call_id")
  @@index([reviewer_id], map: "reviewer_id")
}

model call_proposals {
  id             Int                    @id @default(autoincrement()) @db.UnsignedInt
  last_name      String?                @db.VarChar(255)
  first_name     String?                @db.VarChar(255)
  nationality    Int?                   @db.UnsignedMediumInt
  gender         call_proposals_gender?
  age            Int?                   @db.UnsignedSmallInt
  status         Int?                   @db.UnsignedTinyInt
  domain         String?                @db.VarChar(255)
  diploma        String?                @db.VarChar(255)
  email          String?                @db.VarChar(120)
  mele           String?                @db.VarChar(255)
  university     String?                @db.VarChar(255)
  department     String?                @db.VarChar(255)
  country        Int?                   @db.UnsignedMediumInt
  phd_title      String?                @db.VarChar(255)
  phd_ad_name    String?                @db.VarChar(255)
  phd_year       String?                @db.VarChar(120)
  phd_ad_mail    String?                @db.VarChar(255)
  phd_summary    String?                @db.Text
  cv             String?                @db.VarChar(255)
  paper          String?                @db.VarChar(255)
  reviewer_group Int?                   @db.TinyInt
  created        DateTime               @default(now()) @db.DateTime(0)
  modified       DateTime               @default(now()) @db.DateTime(0)

  @@index([last_name], map: "last_name")
  @@index([reviewer_group], map: "reviewer_group")
}

